{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:prettier/recommended"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": [
    "**/node_modules/**",
    "**/ts.out/**",
    "**/cdk.out/**",
    "**/coverage/**"
  ]
}
